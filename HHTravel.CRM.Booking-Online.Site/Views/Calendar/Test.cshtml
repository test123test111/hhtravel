@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section JS{
    <script type="text/javascript" src="@Url.Content("~/js/hhcal.js")"></script>
    <script type="text/javascript">
        $(function () {
            _url = '@Url.Action("Index")';
            draw(2012, 7);
        });
        
    </script>
}
<div class="calendar_order_warp">
    <h6 class="process">
        <span>1</span>选择出发日期.价格<em>（点击日历上价格可以进行预订，价格显示为<b>双人房每人最低起价</b>）</em></h6>
    <div class="calendar_month">
        <a href="javascript:prevMonth();" class="calendar_l"></a>
        <h5>2012年7月</h5>
        <a href="javascript:nextMonth();" class="calendar_r"></a>
    </div>
    <div class="calendar_warp">
        <table border="0" cellpadding="0" cellspacing="0" class="tbl tbl_calendar">
            <tr class="tbl_header"><td>日</td><td>一</td><td>二</td><td>三</td><td>四</td><td>五</td><td>六</td></tr>
        </table>
    </div>
</div>
